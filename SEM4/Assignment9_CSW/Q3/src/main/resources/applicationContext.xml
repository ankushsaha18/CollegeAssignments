<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="car" class="org.example.Car">
        <property name="name" value="Honda Civic"/>
        <property name="id" value="301"/>
    </bean>
    <bean id="bike" class="org.example.Bike">
        <constructor-arg name="name" value="RE 350"/>
        <constructor-arg name="id" value="350"/>
    </bean>
</beans>